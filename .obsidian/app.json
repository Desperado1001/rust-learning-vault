  "vimMode": false,
  "legacyEditor": false,
  "tabSize": 2,
  "lineWrap": true,
  "indentWithTabs": false,
  "showInlineTitle": true,
  "pdfExportSettings": {
    "pageSize": "A4",
    "landscape": false,
    "margin": "0",
    "downscalePercent": 100
